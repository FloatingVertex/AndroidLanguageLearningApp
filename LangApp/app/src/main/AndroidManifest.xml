<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pushpindersingh.testapp1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MCDefinitionQ"
            android:noHistory="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ExtraInfo"
            android:label="@string/title_activity_extra_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".wordlist"
            android:label="@string/title_activity_wordlist"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".wordslist"
            android:label="@string/title_activity_wordslist"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>